{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "excerpt": {
      "type": "text"
    },
    "content": {
      "type": "blocks"
    },
    "published": {
      "type": "datetime"
    },
    "accessMode": {
      "type": "enumeration",
      "enum": [
        "free",
        "premium",
        "premium_window"
      ]
    },
    "isPublished": {
      "type": "boolean"
    }
  }
}
